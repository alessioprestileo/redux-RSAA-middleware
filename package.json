{
  "name": "redux-rsaa-middleware",
  "version": "1.0.0",
  "description": "Redux middleware for handling API-calling actions.",
  "main": "index.jsx",
  "scripts": {
    "start": "npm run serve:dev",
    "build:dev": "npm run flow && npm run eslint:fix && npm run test && webpack --env.BUILD_TYPE=DEVELOPMENT",
    "build:prod": "npm run flow && npm run eslint:fix && npm run test && webpack --env.BUILD_TYPE=PRODUCTION",
    "eslint:log": "eslint . --ext .js,.jsx",
    "eslint:fix": "eslint . --ext .js,.jsx --fix",
    "flow": "flow",
    "flow-typed-install": "flow-typed install",
    "gulp": "gulp",
    "precommit": "npm run flow && npm run test && lint-staged",
    "sass-lint:log": "sass-lint --verbose",
    "serve:dev": "npm run build:dev -- --watch & node app.dev.js",
    "serve:prod": "npm run build:prod && node app.js",
    "test": "jest --verbose --config ./test/jest.config.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.jsx": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alessioprestileo/redux-RSAA-middleware.git"
  },
  "keywords": [
    "fontend",
    "react"
  ],
  "author": "Alessio Prestileo",
  "license": "See license in LICENSE file",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.4.2",
    "css-loader": "^0.28.9",
    "empty": "^0.10.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.9",
    "flow-bin": "^0.66.0",
    "flow-typed": "^2.3.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-env": "^0.4.0",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jest": "^22.3.0",
    "jest-cli": "^22.4.2",
    "lint-staged": "^6.1.0",
    "node-sass": "^4.7.2",
    "prettier": "^1.10.2",
    "react-hot-loader": "^3.1.3",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "url-loader": "^0.6.2",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.21.0"
  },
  "dependencies": {
    "express": "^4.16.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "superagent": "^3.8.2",
    "webpack": "^3.10.0"
  }
}
